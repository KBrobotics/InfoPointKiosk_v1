    {
        "id": "prep-influxdb",
        "type": "function",
        "z": "f6f2187d.f17ca8",
        "name": "Formatuj dla InfluxDB",
        "func": "// Przygotowanie danych dla węzła InfluxDB\n// Wymaga zainstalowania: node-red-contrib-influxdb\n\nvar data = msg.payload.payload;\n\n// InfluxDB oczekuje obiektu lub tablicy\nmsg.measurement = \"work_logs\";\n\nmsg.payload = [\n    {\n        // Fields (Wartości)\n        action: data.action,\n        department: data.department,\n        employeeName: data.employeeName\n    },\n    {\n        // Tags (Indeksy - do szybkiego wyszukiwania)\n        employeeId: data.employeeId\n    }\n];\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 780,
        "y": 380,
        "wires": [
